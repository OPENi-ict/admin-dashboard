extends layout

block header
	include ./header-navbar.jade

block sidebar
	include ./sidebar-menu.jade

block content
	.col-sm-9.col-sm-offset-3.col-md-10.col-md-offset-2.main

		p
		each c in clients
			h3 Name: #{c.name}
			p  Description: #{c.description}
			p  API Key: #{c.api_key}
			p  Secret Key: #{c.secret}
			a(href="/admin/permissions/#{c.api_key}") Manage App Permissions (TODO)


